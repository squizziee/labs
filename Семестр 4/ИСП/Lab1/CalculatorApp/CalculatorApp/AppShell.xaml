<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CalculatorApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CalculatorApp"
    Title="CalculatorApp">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Calculator"
        ContentTemplate="{DataTemplate local:CalculatorPage}"
        Route="CalculatorPage" />

    <ShellContent
        Title="Integral"
        ContentTemplate="{DataTemplate local:IntegralPage}"
        Route="IntegralPage" />

    <ShellContent
        Title="List"
        ContentTemplate="{DataTemplate local:ListPage}"
        Route="ListPage" />

    <ShellContent
        Title="Rates"
        ContentTemplate="{DataTemplate local:RatePage}"
        Route="RatePage" />

</Shell>
